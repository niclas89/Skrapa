            for (var i = 0; i < targets.length; i++) {
                if (targets[i].getAttribute('data-interchange').indexOf(targetSize) !== -1) {
                    if (targets[i].tagName.toLocaleLowerCase() === "div") {
                        targets[i].style.backgroundImage = window.getComputedStyle(targets[i]).getPropertyValue('background-image').replace("Small50", targetSize);
                    } else if (targets[i].tagName.toLocaleLowerCase() === "img") {
                        targets[i].src = targets[i].src.replace("Small50", targetSize);
                    }
